//SBGOLOB1 JOB (ACCT#),S-GOLOB,
// NOTIFY=&SYSUID,
// CLASS=B,MSGCLASS=X
//*******************************************************************//
//***  SBGOLOB.T508     :    VOL=SER=V508MT                       ***//
//*******************************************************************//
//*  THIS JOB TAKES A DISK MEMBERS OF A PDS AND OFFLOADS THEM TO    *//
//*  A SEQUENTIAL TEMPORARY DATASET.  THE CBT973 PROGRAM COMPRESSES *//
//*  THAT DATA AND THEN WRITES IT TO THE TRANS TAPE FILE.           *//
//*******************************************************************//
//GENR PROC N=001,VL=V508XX,DPFX='CBT.CBT506.',FRX='XXXXXXX',
//      TO='FILE0WWW',LR=80,BL=5600,RF=FB
//GENERIT  EXEC PGM=CKIEBGEN,REGION=3200K         <=== STEP NUMB
//STEPLIB  DD  DISP=SHR,DSN=SYS1.CBT.LINKLIB
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  DSN=&DPFX&FRX,
//             DISP=SHR,
//             DCB=(DSORG=PS,LRECL=&LR,BLKSIZE=&BL,RECFM=&RF)
//SYSUT2   DD  DSN=&TO,
//             DISP=(NEW,KEEP),
//             UNIT=568,
//  DCB=(DSORG=PS,RECFM=&RF,LRECL=&LR,BLKSIZE=&BL,TRTCH=NOCOMP),
//             LABEL=(&N,SL,EXPDT=98000), <=== CHANGE FILE NUMBER
//             VOL=(,RETAIN,SER=&VL)
//SYSIN    DD  DUMMY
//GENR     PEND
//NULL PROC N=001,VL=V508XX,TO='FILE0ZZZ'
//NULFILE   EXEC  PGM=CBT973,PARM=COMPRESS,REGION=1024K  <=== STEP NUMB
//STEPLIB  DD  DISP=SHR,DSN=SYS1.CBT.LINKLIB
//INPUT    DD  DISP=SHR,DSN=SBGOLOB.CBT.EDIT(CBTNULL)
//OUTPUT   DD  DSN=&TO,
//             DISP=(NEW,KEEP),
//             UNIT=568,
//             LABEL=(&N,SL,EXPDT=98000), <=== CHANGE FILE NUMBER
//             VOL=(,RETAIN,SER=&VL),
//             DCB=(RECFM=VB,LRECL=94,BLKSIZE=32716,TRTCH=NOCOMP)
//NULL     PEND
//*
//COP1 PROC N=001,VL=V508XX,DPFX='CBT.CBT506.',FRX='XXXXXXX',
//      TO='FILE0YYY'
//COPYXXXA  EXEC  PGM=IEBCOPY,REGION=3200K         <=== STEP NUMB
//SYSPRINT DD  SYSOUT=*
//SYSUT3   DD  UNIT=SYSALLDA,
//             SPACE=(CYL,(5,5))
//SYSUT4   DD  UNIT=SYSALLDA,
//             SPACE=(CYL,(5,5))
//I        DD  DSN=&DPFX&FRX,
//             DISP=SHR
//J        DD  DSN=&TO,
//             DISP=(NEW,KEEP),
//             UNIT=568,
//             DCB=TRTCH=NOCOMP,
//             LABEL=(&N,SL,EXPDT=98000), <=== CHANGE FILE NUMBER
//             VOL=(,RETAIN,SER=&VL)
//SYSIN    DD   DISP=SHR,DSN=SBGOLOB.CBT508.FILE008(XIJ)
//COP1  PEND
//*
//OFFL PROC N=001,VL=V508XX,DPFX='CBT.CBT506.',FRX='XXXXXXX',
//        TO='FILE0XXX'
//STEPXXX  EXEC PGM=LISTPDP4,REGION=5080K,
//    PARM='UPDTE(><),SPF,ALIAS,NOLIST,DECK,NOSEL'
//STEPLIB  DD DISP=SHR,DSN=SYS1.CBT.LINKLIB
//SYSPRINT DD SYSOUT=*
//SYSUPLOG DD DISP=SHR,DSN=CBT.X508.SYSUPLOG(FILE&N)
//SYSLIB DD  DISP=SHR,DSN=&DPFX&FRX
//SYSPUNCH DD  DSN=&&S23,
//             DISP=(NEW,PASS),
//             UNIT=SYSALLDA,
//             SPACE=(CYL,(40,17),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=23440)
//SYSIN  DD DUMMY
//SYSABEND DD SYSOUT=*
//SYSLIST  DD SYSOUT=*
//*
//STEPYYY EXEC  PGM=CBT973,PARM=COMPRESS,REGION=1024K  <= STEP NUMB
//STEPLIB  DD  DISP=SHR,DSN=SYS1.CBT.LINKLIB
//INPUT    DD  DSN=&&S23,
//             DISP=(OLD,DELETE)
//OUTPUT   DD  DSN=&TO,
//             DISP=(NEW,KEEP),
//             UNIT=568,
//             LABEL=(&N,SL,EXPDT=98000), <=== CHANGE FILE NUMBER
//             VOL=(,RETAIN,SER=&VL),
//             DCB=(RECFM=VB,LRECL=94,BLKSIZE=32716,TRTCH=NOCOMP)
//OFFL  PEND
//*
//OFFG PROC N=001,VL=V508XX,DPFX='CBT.CBT506.',FRX='XXXXXXX',
//        TO='FILE0XXX'
//*
//STEPYYY EXEC  PGM=CBT973,PARM=COMPRESS,REGION=1024K  <= STEP NUMB
//STEPLIB  DD  DISP=SHR,DSN=SYS1.CBT.LINKLIB
//INPUT    DD  DSN=&DPFX.&FRX,
//             DISP=SHR
//OUTPUT   DD  DSN=&TO,
//             DISP=(NEW,KEEP),
//             UNIT=568,
//             LABEL=(&N,SL,EXPDT=98000), <=== CHANGE FILE NUMBER
//             VOL=(,RETAIN,SER=&VL),
//             DCB=(RECFM=VB,LRECL=94,BLKSIZE=32716,TRTCH=NOCOMP)
//OFFG  PEND
//NEWJCL  PROC
//COPY1   EXEC PGM=CKIEBGEN
//SYSPRINT DD  SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=SBGOLOB.CBT.EDIT(FILE001)
//SYSUT2 DD DSN=FILE0001,
//         DISP=(NEW,KEEP),
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=32720,TRTCH=NOCOMP),
//         UNIT=568,
//         LABEL=(1,SL,EXPDT=98000),
//         VOL=(,RETAIN,SER=V508XX)
//SYSIN  DD DUMMY
//COPY2   EXEC PGM=CKIEBGEN
//SYSPRINT DD  SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=SBGOLOB.CBT508.FILE008(##CBT973)
//SYSUT2 DD DSN=FILE0002,
//         DISP=(NEW,KEEP),
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=32720,TRTCH=NOCOMP),
//         UNIT=568,
//         LABEL=(2,SL,EXPDT=98000),
//         VOL=(,RETAIN,SER=V508XX)
//SYSIN  DD DUMMY
//COPY3   EXEC PGM=CKIEBGEN
//SYSPRINT DD  SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=SBGOLOB.CBT508.FILE008($$JCL003)
//SYSUT2 DD DSN=FILE0003,
//         DISP=(NEW,KEEP),
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=32720,TRTCH=NOCOMP),
//         UNIT=568,
//         LABEL=(3,SL,EXPDT=98000),
//         VOL=(,RETAIN,SER=V508XX)
//SYSIN  DD DUMMY
//COPY4   EXEC PGM=CKIEBGEN
//SYSPRINT DD  SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=SBGOLOB.CBT508.FILE008(CBTUPD)
//SYSUT2 DD DSN=FILE0004,
//         DISP=(NEW,KEEP),
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=32720,TRTCH=NOCOMP),
//         UNIT=568,
//         LABEL=(4,SL,EXPDT=98000),
//         VOL=(,RETAIN,SER=V508XX)
//SYSIN  DD DUMMY
//COPY5   EXEC PGM=CKIEBGEN
//SYSPRINT DD  SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=SBGOLOB.CBT508.FILE008(FILE005)
//SYSUT2 DD DSN=FILE0005,
//         DISP=(NEW,KEEP),
//         DCB=(RECFM=FB,LRECL=80,BLKSIZE=32720,TRTCH=NOCOMP),
//         UNIT=568,
//         LABEL=(5,SL,EXPDT=98000),
//         VOL=(,RETAIN,SER=V508XX)
//SYSIN  DD DUMMY
//         PEND
//S00  EXEC NEWJCL                                      - FILES 1-5
//S06  EXEC OFFL,N=006,TO='FILE0006',FRX='FILE006.PDS'
//S07  EXEC GENR,N=007,TO='FILE0007',FRX='CBT508.FILE008(FILE007)',
//             LR=80,BL=5600,RF=FB,DPFX='SBGOLOB.'
//S08  EXEC OFFL,N=008,TO='FILE0008',FRX='CBT508.FILE008',
//     DPFX='SBGOLOB.'
//S09  EXEC OFFL,N=009,TO='FILE0009',FRX='FILE009.PDS'
//S10  EXEC OFFL,N=010,TO='FILE0010',FRX='FILE010.PDS'
//S11  EXEC OFFL,N=011,TO='FILE0011',FRX='FILE011.PDS'
//S12  EXEC OFFL,N=012,TO='FILE0012',FRX='FILE012.PDS'
//S13  EXEC OFFL,N=013,TO='FILE0013',FRX='FILE013.PDS'
//S14  EXEC OFFL,N=014,TO='FILE0014',FRX='FILE014.PDS'
//S15  EXEC OFFL,N=015,TO='FILE0015',FRX='FILE015.PDS'
//S16  EXEC OFFL,N=016,TO='FILE0016',FRX='FILE016.PDS'
//S17  EXEC OFFL,N=017,TO='FILE0017',FRX='FILE017.PDS'
//S18  EXEC GENR,N=018,TO='FILE0018',FRX='FILE018.DATA',
//             LR=133,BL=6118,RF=FBA
//S19  EXEC OFFL,N=019,TO='FILE0019',FRX='FILE019.PDS'
//S20  EXEC OFFL,N=020,TO='FILE0020',FRX='FILE020.PDS'
//S21  EXEC OFFL,N=021,TO='FILE0021',FRX='FILE021.PDS'
//S22  EXEC OFFL,N=022,TO='FILE0022',FRX='FILE022.PDS'
//S23  EXEC OFFL,N=023,TO='FILE0023',FRX='FILE023.PDS'
//S24  EXEC OFFL,N=024,TO='FILE0024',FRX='FILE024.PDS'
//S25  EXEC OFFL,N=025,TO='FILE0025',FRX='FILE025.PDS'
//S26  EXEC OFFL,N=026,TO='FILE0026',FRX='FILE026.PDS'
//S27  EXEC OFFL,N=027,TO='FILE0027',FRX='FILE027.PDS'
//S28  EXEC OFFL,N=028,TO='FILE0028',FRX='FILE028.PDS'
//S29  EXEC OFFL,N=029,TO='FILE0029',FRX='FILE029.PDS'
//S30  EXEC OFFL,N=030,TO='FILE0030',FRX='FILE030.PDS'
//S31  EXEC OFFL,N=031,TO='FILE0031',FRX='FILE031.PDS'
//S32  EXEC OFFL,N=032,TO='FILE0032',FRX='FILE032.PDS'
//S33  EXEC OFFL,N=033,TO='FILE0033',FRX='FILE033.PDS'
//S34  EXEC OFFL,N=034,TO='FILE0034',FRX='FILE034.PDS'
//S35  EXEC COP1,N=035,TO='FILE0035',FRX='FILE035.PDS'
//S36  EXEC OFFL,N=036,TO='FILE0036',FRX='FILE036.PDS'
//S37  EXEC OFFL,N=037,TO='FILE0037',FRX='FILE037.PDS'
//S38  EXEC OFFL,N=038,TO='FILE0038',FRX='FILE038.PDS'
//S38  EXEC OFFL,N=039,TO='FILE0039',FRX='FILE039.PDS'
